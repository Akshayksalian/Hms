package com.hms.service;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

import com.hms.dao.QuestionaireDAO;
import com.hms.entities.Questionaire;

@Service
@Transactional(readOnly = true)
public class QuestionaireServiceImpl implements QuestionaireService{

	@Autowired
	private QuestionaireDAO questionaireDAO;
	
	@Override
	@Transactional
	public void addAnswer(Questionaire questionaire) {
		questionaireDAO.addAnswer(questionaire);
	}

}
